        "/auth/forgot-password": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Kirim link reset password",
                "description": "Mengirim email berisi link untuk mereset password pengguna.",
                "operationId": "forgotPassword",
                "requestBody": {
                    "description": "Email pengguna yang terdaftar",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [

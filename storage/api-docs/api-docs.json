    "components": {
        "schemas": {
            "AdminCreateRequest": {
                "required": [
                    "username",
                    "email",
                    "password",
                    "admin_name",
                    "admin_phone"
                ],
                "properties": {
                    "username": {
                        "type": "string",
                        "example": "superadmin"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "super.admin@example.com"
                    },
                    "password": {
                        "description": "Min. 8 karakter, 1 huruf besar, 1 angka, 1 simbol",
                        "type": "string",
                        "format": "password",
                        "example": "Password123!"
                    },
                    "admin_name": {
                        "type": "string",
                        "example": "Super Admin"
                    },
                    "admin_phone": {
                        "type": "string",
                        "example": "081234567890"
                    }
                },
                "type": "object"
            },
            "AdminUpdateRequest": {
                "description": "Semua field bersifat opsional. Hanya field yang dikirim yang akan diupdate.",
                "properties": {
                    "username": {
                        "type": "string",
                        "example": "superadmin_updated"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "super.admin.new@example.com"
                    },
                    "password": {
                        "description": "Kirim hanya jika ingin mengubah password",
                        "type": "string",
                        "format": "password",
                        "example": "NewPassword123!"
                    },
                    "admin_name": {
                        "type": "string",
                        "example": "Super Admin Name Updated"
                    },
                    "admin_phone": {
                        "type": "string",
                        "example": "081234567891"
                    }
                },
                "type": "object"
            },
            "RegistrationRequest": {
                "required": [
                    "child_name",
                    "child_gender",
                    "child_birth_place",
                    "child_birth_date",
                    "child_address",
                    "child_complaint",
                    "child_service_choice",
                    "email",
                    "guardian_name",
                    "guardian_phone",
                    "guardian_type"
                ],
                "properties": {
                    "child_name": {
                        "type": "string",
                        "example": "Citra Lestari"
                    },
                    "child_gender": {
                        "type": "string",
                        "enum": [
                            "Laki-laki",
                            "Perempuan"
                        ],
                        "example": "Perempuan"
                    },
                    "child_birth_place": {
                        "type": "string",
                        "example": "Surakarta"
                    },
                    "child_birth_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2018-05-20"
                    },
                    "child_school": {
                        "type": "string",
                        "example": "SDN 1 Pagi",
                        "nullable": true
                    },
                    "child_address": {
                        "type": "string",
                        "example": "Jl. Merdeka No. 10"
                    },
                    "child_complaint": {
                        "type": "string",
                        "example": "Sulit berkonsentrasi saat belajar"
                    },
                    "child_service_choice": {
                        "type": "string",
                        "example": "Terapi Okupasi"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "wali.baru@example.com"
                    },
                    "guardian_name": {
                        "type": "string",
                        "example": "Budi Santoso"
                    },
                    "guardian_phone": {
                        "type": "string",
                        "example": "081234567890"
                    },
                    "guardian_type": {
                        "type": "string",
                        "enum": [
                            "ayah",
                            "ibu",
                            "wali"
                        ],
                        "example": "ayah"
                    }
                },
                "type": "object"
            },
            "ResetPasswordRequest": {
                "required": [
                    "token",
                    "email",
                    "password",
                    "password_confirmation"
                ],
                "properties": {
                    "token": {
                        "description": "Token yang diterima dari email",
                        "type": "string"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "john.doe@example.com"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "example": "NewPassword123!"
                    },
                    "password_confirmation": {
                        "type": "string",
                        "format": "password",
                        "example": "NewPassword123!"
                    }
                },
                "type": "object"
            },
            "TherapistCreateRequest": {
                "required": [
                    "username",
                    "email",
                    "password",
                    "therapist_name",
                    "therapist_section",
                    "therapist_phone"
                ],
                "properties": {
                    "username": {
                        "type": "string",
                        "example": "dr.johndoe"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "dr.johndoe@example.com"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "example": "Password123!"
                    },
                    "therapist_name": {
                        "type": "string",

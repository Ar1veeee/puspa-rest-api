                    },
                    "therapist_phone": {
                        "type": "string",
                        "example": "081234567891"
                    }
                },
                "type": "object"
            },
            "UserLoginRequest": {
                "required": [
                    "identifier",
                    "password"
                ],
                "properties": {
                    "identifier": {
                        "description": "Bisa berupa username atau email",
                        "type": "string",
                        "example": "johndoe"
                    },
                    "password": {
